@base <https://touhou-api.org/resources/> .

@prefix touhou: <https://touhou-api.org/schema/> .

@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#>.

# Classes

touhou:Character        a   rdfs:Class;
    rdfs:label              "キャラクター"@ja;
    rdfs:comment            "キャラクターを表すクラス";
    .

touhou:Game             a   rdfs:Class;
    rdfs:label              "ゲーム"@ja, "Game"@en;
    rdfs:comment            "ゲームを表すクラス";
    .

touhou:Music            a   rdfs:Class;
    rdfs:label              "音楽"@ja;
    rdfs:comment            "音楽を表すクラス";
    .

touhou:Music            a   rdfs:Class;
    rdfs:label              "音楽"@ja;
    rdfs:comment            "音楽を表すクラス";
    .

touhou:Species          a   rdfs:Class;
    rdfs:label              "種族"@ja;
    rdfs:comment            "種族を表すプロパティ";
    .

touhou:Generation       a   rdfs:Class;
    rdfs:label              "シリーズ"@ja, "Generation"@en;
    rdfs:comment            "TODO:現状では旧作かWindowsかのどっちかです";
    .

touhou:Role             a   rdfs:Class;
    rdfs:label              "役割"@ja, "Role"@en;
    .

touhou:CharacterTitle   a   rdfs:Class;
    rdfs:label              "二つ名"@ja, "Character Title"@en;
    .

# Properties

touhou:GameNumber           a   rdfs:Property;
    rdfs:label                  "ゲームのナンバー";
    rdfs:comment                "ゲームのナンバーを表すプロパティ";
    .

touhou:GameAbbreviation     a   rdfs:Property;
    rdfs:label                  "ゲームの略称";
    rdfs:comment                "ゲームの略称を表すプロパティ";
    .

touhou:GameRole             a   rdfs:Property;
    rdfs:label                  "ゲーム上での役割";
    rdfs:comment                "ゲーム上での役割（自機、ボスなど）を表すプロパティ";
    .

touhou:NameKana             a   rdfs:Property;
    rdfs:label                  "名前のよみがな";
    rdfs:comment                "名前のよみがなを表すプロパティ";
    .

touhou:NamePronunciation    a   rdfs:Property;
    rdfs:label                  "名前の発音";
    rdfs:comment                "名前の発音を表すプロパティ";
    .

touhou:MusicThemeOf         a   rdfs:Character;
    rdfs:label                  "誰のテーマ曲か";
    rdfs:comment                "誰のテーマ曲かを表すプロパティ";
    .

touhou:CharacterOf          a   rdfs:Property;
    .
